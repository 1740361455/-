#include<stdio.h>
int main() {
	int x=1201,x1=x;
	long y=0;
	if(x<0||(x%10==0&&x!=0))
  {
  	printf("false");
    return 0;
   }
	while(x1)
	{
		y=x1%10+y*10;
		x1/=10;
	}
	if(x==y)
	printf("true");
	else
	printf("false");
}









#include<stdio.h>
int main() {
	int x=121,y=0;
	if(x<0||(x%10==0&&x!=0))
	{
		printf("false");
		return 0;
	}
	while(x>y)
	{
		y=x%10+y*10;
		x/=10;
	}
	if(x<y)
	y/=10;
	if(x==y)
	printf("true");
	else
	printf("false");
}
