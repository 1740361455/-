#include<stdio.h>
int main()
{
    int i,n,h,a,b,c;
    scanf("%d",&n);
    b=n;
    h=1;
    for(i=1;i<=10;i++)
    {
        if(n==0)
        continue;
        n=n/10;
        h=h*10;
    }
    h=h/10;
    for(a=1;a<=10;a++)
    {
        if(h==0)
        continue;
        c=b/h;
        b=b-c*h;
        h=h/10;
        printf("%d ",c);
    }
    return 0;
}
