   #include <stdio.h>
   #include <math.h>
   int main(){ 
   double a,b,c,delta,x1,x2;
	printf("请输入一下各数的值a,b,c:\n");
	scanf("%lf%lf%lf",&a,&b,&c);
	delta=b*b-4.0*a*c;
	if(delta>=0){
	x1=(-b+sqrt(delta))/(2.0*a);
	x2=(-b-sqrt(delta))/(2.0*a);
	printf("x1=%lf,x2=%lf",x1,x2);
	}
	else
	printf("无解");
	return 0;
}
