#include<stdio.h>
#include<string.h>
void reverse(char* arr,char* brr)
{
	char* left=arr;
	char* right=brr;
	char tmp;
	while(left<right)
	{
		tmp=*left;
		*left=*right;
		*right=tmp;
		left++;
		right--;
	}
}
void left_move(char* arr,int num)
{
	int sz=strlen(arr);
	reverse(arr,arr+num-1);
	reverse(arr+num,arr+sz-1);
	reverse(arr,arr+sz-1);
}
int is_left_move(char* arr,char* brr,int str)
{
	int i,j=0;
	for(i=0;i<str-1;i++)
	{
		left_move(arr,1);
		if(j==strcmp(arr,brr))
		return 0;
	}
	return 1;
}
int main()
{
	char arr[]="abcde",brr[]="bcdea";
	int str=strlen(arr);
	int judge=is_left_move(arr,brr,str);
	if(judge==0)
	printf("yes");
	else
	printf("no");
	return 0;
}
