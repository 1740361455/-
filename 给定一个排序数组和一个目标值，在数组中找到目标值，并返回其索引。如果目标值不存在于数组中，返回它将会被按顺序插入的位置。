#include<stdio.h>
int main()
{
	int nums[]={1,3,5,6};
	int numsSize=4;
	int target=7;
	for(int i=0;i<numsSize;i++)
	{
    if((i==0&&nums[i]>target)||nums[i]==target)
    {
		 printf("%d",i);
		 break;
	}
    else if((i==numsSize-1&&nums[i]<target)||(nums[i]<target&&nums[i+1]>target))
   {
   	printf("%d",i+1);
   	break;
   }
   }
	return 0;
}
