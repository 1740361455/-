#include<stdio.h>
int main() {
	int arr[] = {1, 3, 5, 6};
	int sz = sizeof(arr) / sizeof(arr[0]), left = 0, right = sz - 1, tmp, target = 7;
	while (left <= right) {
		tmp = (left + right) / 2;
		if (target > arr[tmp])
			left = tmp + 1;
		else if (target < arr[tmp])
			right = tmp - 1;
		else {
			printf("%d", tmp);
			return 0;
		}
	}
	if (tmp == 0 && arr[tmp] > target)
		printf("0");
	else if (tmp == sz - 1 && arr[tmp] < target)
		printf("%d", sz);
	else if (arr[tmp] > target)
		printf("%d", tmp);
	else
		printf("%d", tmp + 1);
	return 0;
}
