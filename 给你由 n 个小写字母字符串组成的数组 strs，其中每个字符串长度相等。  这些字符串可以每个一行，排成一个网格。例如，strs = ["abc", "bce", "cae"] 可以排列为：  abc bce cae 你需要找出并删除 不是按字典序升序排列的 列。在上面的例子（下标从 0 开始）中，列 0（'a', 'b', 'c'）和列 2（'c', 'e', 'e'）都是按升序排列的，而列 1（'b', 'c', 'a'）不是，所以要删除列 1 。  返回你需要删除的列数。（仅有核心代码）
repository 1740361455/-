int minDeletionSize(char ** strs, int strsSize){
    int count=0;
    int sz=strlen(strs[0]);
    for(int i=0;i<sz;i++)
    {
        for(int j=0;j<strsSize-1;j++)
        {
            if(strs[j][i]>strs[j+1][i])
            {
                count++;
                break;
            }
        }
    }
    return count;
}
