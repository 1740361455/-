#include <stdio.h>
int main()
{
	int n,a,b,c,i;
	float num;
	while(1)
	{
		scanf("%d",&n);
		if(n==0)
			break;
		else
		{
			a=0,b=0,c=0;
			for(i=1;i<=n;i++)
			{
				scanf("%f",&num);
				if(num==0)
					b++;
				else if(num<0)
					a++;
				else
					c++;
			}
			printf("%d %d %d",a,b,c);
			printf("\n");
		}
	}
	return 0;
}
