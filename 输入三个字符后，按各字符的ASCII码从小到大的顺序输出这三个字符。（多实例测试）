 #include <stdio.h>
int main()
{
    char a,b,c,t;
    while(scanf("%c%c%c",&a,&b,&c)!=EOF)
    {
        scanf("\n");
        if(b>a)
        {
            t=a;
            a=b;
            b=t;
        }
        if(c>b)
        {
            t=b;
            b=c;
            c=t;
        }
        if(b>a)
        {
            t=a;
            a=b;
            b=t;
        }
        printf("%c %c %c",c,b,a);
        printf("\n");
    }
    return 0;
}
