#include <stdio.h>
void bubble_sort(int arr[],int num)
{
	int i,j,tmp;
	for(i=0;i<num-1;i++)
	{
		for(j=0;j<num-1-i;j++)
		{
			if(arr[j]>arr[j+1])
			{
				tmp=arr[j+1];
				arr[j+1]=arr[j];
				arr[j]=tmp;
			}
		}
	}
}
int main()
{
	int arr[]={9,8,7,6,5,4,3,2,1},num,i;
	num=sizeof(arr)/sizeof(arr[0]);
	bubble_sort(arr,num);
	for(i=0;i<num;i++)
	printf("%d ",arr[i]);
	return 0;
}
